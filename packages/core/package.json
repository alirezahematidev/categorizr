{
  "name": "@treekit/core",
  "version": "0.0.2",
  "type": "module",
  "license": "MIT",
  "author": "Alireza Hemati",
  "description": "A superpower tool to manage and handle nested tree data in javascript",
  "keywords": [
    "tree",
    "category",
    "hierarchy"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/core.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/core.es.js"
      }
    },
    "./functions": {
      "import": {
        "types": "./dist/functions/index.d.ts",
        "default": "./dist/functions.es.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alirezahematidev/treekit.git"
  },
  "bugs": {
    "url": "https://github.com/alirezahematidev/treekit/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/alirezahematidev/treekit#readme",
  "scripts": {
    "build": "tsc && vite build"
  },
  "devDependencies": {
    "vite-plugin-dts": "3.9.1",
    "rollup-plugin-esbuild": "6.1.1",
    "rollup": "4.17.2",
    "esbuild": "0.21.2"
  }
}
